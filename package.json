{
  "name": "seventyfive-api",
  "version": "1.0.0",
  "description": "Seventy Five Tennis Club API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node src/app.ts",
    "dev": "npx nodemon src/app.ts",
    "build": "npx tsc && cp package.json dist/ && cp .env dist/",
    "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli -d ./src/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./src/database/migrations/migration",
    "migrate": "npm run typeorm -- migration:run",
    "migrate:status": "npm run typeorm -- migration:show",
    "migrate:revert": "npm run typeorm -- migration:revert && npm run migrate:status",
    "db:seed": "ts-node ./src/database/seed/index.ts",
    "seed:category": "ts-node src/database/seed/merch-category.seed.ts",
    "seed:provinces": "ts-node src/database/seed/provinces.seed.ts"
  },
  "author": "zainulkarim",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/newrelic": "^9.14.4",
    "@types/node": "^20.12.13",
    "@types/uuid": "^9.0.8",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-group-routes": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "newrelic": "^12.5.1",
    "nodemon": "^3.0.2",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "undici-types": "^6.6.2",
    "uuid": "^9.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
